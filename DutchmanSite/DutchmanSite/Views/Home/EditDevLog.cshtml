
@{
    ViewBag.Title = "EditDevLog";
    Layout = "~/Views/Shared/_Layout.cshtml";
    string ThisLog = Path.GetFileName(Request.Url.ToString());
    DutchmanSite.Models.DevLogsModel Logs = new DutchmanSite.Models.DevLogsModel();

}
@Html.Raw(ThisLog)
<form action="" method="post" enctype="multipart/form-data" class="form-horizontal">
    <fieldset>
        @Html.Raw(Logs.LogEdit(Convert.ToInt64(ThisLog)))
        <div class='form-group'>
            <label class='col-md-4 control-label' for='save'></label>
            <div class='col-md-8'>
                <button type='submit' class='btn btn-info'>Save</button>
                <button data-dismiss='modal' class='btn btn-danger'>Exit</button>
            </div>
        </div>
    </fieldset>
</form>